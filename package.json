{
  "name": "blockplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "dotenv -e src/config/env/.development.env -- nest start",
    "start:dev": "dotenv -e src/config/env/.development.env -- nest start --watch",
    "start:debug": "dotenv -e src/config/env/.development.env -- nest start --debug --watch",
    "start:prod": "dotenv -e dist/config/env/.production.env -- node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "dotenv -e src/config/env/.test.env jest -i --runInBand --detectOpenHandles",
    "test:watch": "dotenv -e src/config/env/.test.env -- jest --watch --runInBand --detectOpenHandles",
    "test:cov": "dotenv -e src/config/env/.test.env -- jest --coverage",
    "test:debug": "dotenv -e src/config/env/.test.env -- node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv -e src/config/env/.test.env -- jest --config ./test/jest-e2e.json",
    "db:dev:push": "dotenv -e src/config/env/.development.env -- npx prisma db push",
    "db:dev:pull": "dotenv -e src/config/env/.development.env -- npx prisma db pull",
    "db:dev:generate": "dotenv -e src/config/env/.development.env -- npx prisma generate",
    "db:prod:push": "dotenv -e dist/config/env/.production.env -- npx prisma db push",
    "db:prod:pull": "dotenv -e dist/config/env/.production.env -- npx prisma db pull",
    "db:prod:generate": "dotenv -e dist/config/env/.production.env -- npx prisma generate"
  },
  "dependencies": {
    "@nestjs/axios": "^1.0.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.1.4",
    "@prisma/client": "^4.12.0",
    "@types/bcrypt": "^5.0.0",
    "aws-sdk": "^2.1292.0",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "express-basic-auth": "^1.2.1",
    "express-session": "^1.17.3",
    "joi": "^17.7.0",
    "nest-winston": "^1.8.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "ulid": "^2.3.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/jest": "28.1.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.2",
    "prettier": "^2.3.2",
    "prisma": "^4.9.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
